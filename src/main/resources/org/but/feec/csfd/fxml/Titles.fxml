<VBox minHeight="625.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.but.feec.csfd.controller.title.TitlesController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="Help, I need somebody help!" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <SplitPane focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <items>
        <ScrollPane prefHeight="-1.0" prefWidth="-1.0">
          <content>
            <AnchorPane id="Content" maxWidth="1.7976931348623157E308" minHeight="-1.0" minWidth="-Infinity" prefHeight="517.0" prefWidth="943.0">
              <children>
                <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="27.0" prefWidth="147.0" style="&#10;" text="Titles" textAlignment="CENTER" wrapText="false">
                           <font>
                      <Font size="18.0" fx:id="x1" />
                           </font>
                           <textFill>
                      <Color red="0.624" green="0.624" blue="0.624" fx:id="x2" />
                           </textFill></Label>
                        <TableView fx:id="systemTitlesTableView" layoutY="67.0" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="400.0" prefWidth="948.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="-356.0">
                          <columns>
                              <TableColumn fx:id="titlesId" minWidth="75.0" prefWidth="80.00000303983688" text="id" />
                              <TableColumn fx:id="titlesName" minWidth="75.0" prefWidth="170.0" text="title" />
                              <TableColumn fx:id="titlesType" minWidth="47.0" prefWidth="179.0" text="type" />
                              <TableColumn fx:id="titlesYear" minWidth="34.0" prefWidth="172.0" text="year" />
                              <TableColumn fx:id="titlesLenght" minWidth="46.0" prefWidth="190.0" text="lenght" />
                              <TableColumn fx:id="titlesCountry" minWidth="75.0" prefWidth="157.0" text="country" />
                          </columns>
                        </TableView>
                        <Button fx:id="addTitleButton" layoutX="25.0" layoutY="490.0" mnemonicParsing="false" onAction="#handleAddTitleButton" text="Add title" />
                        <Button fx:id="refreshButton" layoutX="125.0" layoutY="490.0" mnemonicParsing="false" onAction="#handleRefreshButton" text="Refresh" />
                        <Button fx:id="usersButton" layoutX="625.0" layoutY="490.0" mnemonicParsing="false" onAction="#handleUsersButton" prefHeight="25.0" prefWidth="145.0" text="&lt;&lt; Users table" />
                        <Button fx:id="dummyButton" layoutX="785.0" layoutY="490.0" mnemonicParsing="false" onAction="#handleDummyButton" prefHeight="25.0" prefWidth="145.0" text="Dummy table &gt;&gt;" />
              </children>
            </AnchorPane>
          </content>
        </ScrollPane>
      </items>
    </SplitPane>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
